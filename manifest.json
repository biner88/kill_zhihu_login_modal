      "default_icon": {
         "48": "assets/icon.png",
         "32": "assets/icon.png",
         "24": "assets/icon.png"
      },
      "default_title": "__MSG_ExtName__"
   },
   "content_scripts": [
      {
         "js": [
            "scripts/main.js"
         ],
         "matches": [
            "*://*.zhihu.com/*"
         ],
         "run_at": "document_idle"
      }
   ],
   "description": "__MSG_ExtDescription__",
